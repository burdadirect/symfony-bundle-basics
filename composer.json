{
  "name": "burdadirect/symfony-bundle-basics",
  "description": "HBM Basics Bundle - Shared functionality (templates, controller, ...) for symfony projects.",
  "authors" : [{
    "name" : "Christian Puchinger",
    "email" : "puchinger@burda.com"
  }],
  "license": "MIT",
  "type": "symfony-bundle",
  "autoload": {
    "psr-4": {
      "HBM\\BasicsBundle\\": "src/"
    }
  },
  "require": {
    "php" : "^8.1.0",
    "ext-json": "*",
    "ext-iconv": "*",
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "^2.0",
    "doctrine/doctrine-fixtures-bundle": "^3.0",
    "fakerphp/faker": "^1.21",
    "liip/test-fixtures-bundle": "^2.0",
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "^5.4",
    "symfony/config": "^5.4",
    "symfony/dependency-injection": "^5.4",
    "symfony/form": "^5.4",
    "symfony/framework-bundle": "^5.4",
    "symfony/http-kernel": "^5.4",
    "symfony/monolog-bundle": "^3.4",
    "symfony/security-bundle": "^5.4",
    "symfony/twig-bundle": "^5.4",
    "symfony/validator": "^5.4",
    "symfony/yaml": "^5.4"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
